<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="HRF.dto">
    <class name="Record" table="recordtab">
        <id name="uniqueId" column="recordid"/>
        <property name="studentName" column="studentname"/>
        <property name="date" column="recorddate"/>
        <property name="day" column="day"/>
        <property name="fromTime" column="recordfromtime"/>
        <property name="toTime" column="recordtotime"/>
        <property name="description" column="recorddescription"/>
    </class>
</hibernate-mapping>
