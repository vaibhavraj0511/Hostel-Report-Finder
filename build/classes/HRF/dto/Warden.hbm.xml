<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="HRF.dto">
    <class name="Warden" table="wardentab">
        <id name="id" column="wardenid"/>
        <property name="fullName" column="wardenname"/>
        <property name="password" column="wardenpassword"/>
        <property name="mobileNo" column="wardenmobileno"/>
        <property name="address" column="wardenaddress"/>
    </class>
</hibernate-mapping>
